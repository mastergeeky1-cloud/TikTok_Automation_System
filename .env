# TikTok Automation System - Production Configuration
# Domain Configuration
GM_DOMAIN=[your-domain.com]
GM_API_BASE=https://api.[your-domain.com]
GM_N8N_URL=https://automation.[your-domain.com]

# AI Services Configuration
OPENAI_API_KEY=your_openai_api_key_here
STABLE_DIFFUSION_URL=http://localhost:7860

# Database Configuration
POSTGRES_PASSWORD=secure_password_change_me
REDIS_HOST=localhost
REDIS_PORT=6379

# Logging Configuration
LOG_LEVEL=INFO

# Security Configuration
MASTER_PASSWORD=1507
JWT_SECRET=your_jwt_secret_change_me

# TikTok API Configuration
TIKTOK_CLIENT_KEY=your_tiktok_client_key
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret

# Service Ports
N8N_PORT=5678
API_PORT=3000
DASHBOARD_PORT=8080

# Content Generation Settings
MAX_VIDEO_DURATION=60
VIDEO_QUALITY=high
AUTO_GENERATE_CAPTIONS=true

# Monitoring Settings
METRICS_COLLECTION_INTERVAL=60
ALERT_THRESHOLDS_CPU=80
ALERT_THRESHOLDS_MEMORY=85
ALERT_THRESHOLDS_DISK=90
